<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome!{% endblock %}</title>
        <link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
        {# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
        <!-- Custom fonts for this template-->
    <link href="{{asset('admin/vendor/fontawesome-free/css/all.min.css')}}" rel="stylesheet" type="text/css">
    <link
        href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
        rel="stylesheet">
       
     <!-- Custom styles for this template-->
      <link href="{{asset('admin/css/sb-admin-2.min.css')}}" rel="stylesheet">

        {% block stylesheets %}
            {{ encore_entry_link_tags('app') }}
        {% endblock %}

        
    </head>
    <body id="page-top">
        <!-- Page Wrapper -->
		<div id="wrapper">

			            {% include "layout/_admin_dashbord.html.twig" %}
			<!-- Content Wrapper -->
			<div id="content-wrapper" class="d-flex flex-column">

				<!-- Main Content -->
				<div id="content">

					{% include "layout/_admin_header.html.twig" %}

					<!-- Logout -->
					{% include "layout/_logout_modal.html.twig" %}
					<!-- End of Topbar -->

					<!-- Begin Page Content -->
						<div class="container-fluid">{% block body %}{% endblock %} </div>
						<!-- /.container-fluid -->

					</div>
					<!-- End of Main Content -->
					<!-- Footer -->
					{% include "layout/_admin_footer.html.twig" %}
					<!-- End of Footer -->
                </div>
			</div>
				<!-- End of Content Wrapper -->

		</div>
         <!-- Scroll to Top Button-->
    	{% include "layout/_scroll_top.html.twig" %}
         <!-- Logout Modal-->
         {% include "layout/_logout_modal.html.twig" %}
    </body>

    <!-- Bootstrap core JavaScript-->
    <script src="{{asset('admin/vendor/jquery/jquery.min.js')}}"></script>
    <script src="{{asset('admin/vendor/bootstrap/js/bootstrap.bundle.min.js')}}"></script>

    <!-- Core plugin JavaScript-->
    <script src="{{asset('admin/vendor/jquery-easing/jquery.easing.min.js')}}"></script>

    <!-- Custom scripts for all pages-->
    <script src="{{asset('admin/js/sb-admin-2.min.js')}}"></script>

    <!-- Page level plugins -->
    <script src="{{asset('admin/vendor/chart.js/Chart.min.js')}}"></script>

    <!-- Page level custom scripts -->
    <script src="{{asset('admin/js/demo/chart-area-demo.js')}}"></script>
    <script src="{{asset('admin/js/demo/chart-pie-demo.js')}}"></script>
    {% block javascripts %}
            {{ encore_entry_script_tags('app') }}
    {% endblock %}
</html>
